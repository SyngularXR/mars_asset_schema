                    "x": 2,
                    "y": 0,
                    "z": 1
                }
            ]
        },
        "VolumeFloatRange": {
            "title": "VolumeFloatRange",
            "description": "Represent the volume range values (Unit: M3)\n\n    ",
            "type": "object",
            "properties": {
                "min": {
                    "title": "The lower threshold of the Range",
                    "minimum": 0,
                    "maximum": 1,
                    "type": "number"
                },
                "max": {
                    "title": "The upper threshold of the Range",
                    "minimum": 0,
                    "maximum": 1,
                    "type": "number"
                }
            },
            "required": [
                "min",
                "max"
            ],
            "examples": [
                {
                    "min": 0.0001,
                    "max": 1
                }
            ]
        },
        "ThresholdRange": {
            "title": "ThresholdRange",
            "description": "Represent the threshold range values (Region of interest)\n\n    ",
            "type": "object",
            "properties": {
                "min": {
                    "title": "The lower threshold of the Range",
                    "minimum": -10000,
                    "maximum": 10000,
                    "type": "integer"
                },
                "max": {
                    "title": "The upper threshold of the Range",
                    "minimum": -10000,
                    "maximum": 10000,
                    "type": "integer"
                }
            },
            "required": [
                "min",
                "max"
            ],
            "examples": [
                {
                    "min": 0,
                    "max": 3000
                }
            ]
        },
        "RemeshModifier": {
            "title": "RemeshModifier",
