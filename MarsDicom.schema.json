                }
            ]
        },
        "MarsDicomMeshModel": {
            "title": "MarsDicomMeshModel",
            "type": "object",
            "properties": {
                "is_enabled": {
                    "title": "Represent the flag of generating volume mesh.",
                    "default": false,
                    "type": "boolean"
                },
                "is_enabled_texture": {
                    "title": "Represent the flag of baking mesh textures with studio lighting scene.",
                    "default": true,
                    "type": "boolean"
                },
                "is_image_coordinate": {
                    "title": "Represent the flag of ignoring the dicom origin (patient coordinate).",
                    "default": true,
                    "type": "boolean"
                },
                "high_res_mesh_face_num": {
                    "title": "Represent the maximum triangle face numbers of the high-resolution mesh (for baking textures).",
                    "default": 300000,
                    "type": "integer"
                },
                "mesh_face_num": {
                    "title": "Represent the maximum triangle face numbers of the export mesh.",
                    "default": 200000,
                    "type": "integer"
                },
                "mesh_min_component_ratio": {
                    "title": "Represent ratio of the min isolated connected components comparing to the total triangle numbers.",
                    "default": 0.05,
                    "type": "number"
                },
                "smooth_level": {
                    "title": "Represent the smooth level. Higher value may lose some details on the mesh.",
                    "default": 2,
                    "type": "integer"
                },
                "volume_threshold": {
                    "title": "Represent the minimum / maximum volume threshold of the model (cubic meter)",
                    "default": {
                        "min": 0.0001,
                        "max": 0.1
                    },
                    "allOf": [
                        {
                            "$ref": "#/definitions/VolumeFloatRange"
                        }
                    ]
                },
                "threshold": {
                    "title": "Intensity threshold Values to build the mesh.",
                    "description": "If the threshold range was invalid, it would calculate the values from the dependent segmentation",
                    "default": {
                        "min": 8,
                        "max": 256
                    },
                    "allOf": [
                        {
                            "$ref": "#/definitions/ThresholdRange"
                        }
                    ]
                },
                "modifiers": {
                    "title": "Represent the modifiers of generating mesh.",
                    "description": "Building segmentation mesh would involve various Mesh Modifiers",
                    "default": [],
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/MeshModifier"
                    }
                }
            },
            "examples": {
                "is_enabled": true,
                "is_dominant": false,
                "is_enabled_texture": true,
                "high_res_mesh_face_num": 500000,
                "mesh_face_num": 300000,
                "mesh_min_component_ratio": 0.05,
                "smooth_level": 1,
                "threshold": {
                    "min": 8,
                    "max": 256
                }
            }
        },
        "SITKFilterItems": {
            "title": "SITKFilterItems",
            "description": "An enumeration.",
            "enum": [
                "DiscreteGaussianImageFilter",
                "UnsharpMaskImageFilter",
                "EnhanceVolumeImageFilter",
                "DenoisingVolumeImageFilter",
                "SyngularVolumeImageFilter",
                "BSRGanVolumeImageFilter"
            ]
        },
        "SITKFilter": {
            "title": "SITKFilter",
            "type": "object",
            "properties": {
                "name": {
                    "title": "SimpleITK Image Filter Name",
                    "description": "The ImageFilter module contains definitions for a pre-defined set of filters.\n>DiscreteGaussianImageFilter: Blurs an image by separable convolution with discrete gaussian kernels. This filter performs Gaussian blurring by separable convolution of an image and a discrete Gaussian operator (kernel).\n>UnsharpMaskImageFilter: Edge enhancement filter.\n",
                    "allOf": [
                        {
                            "$ref": "#/definitions/SITKFilterItems"
                        }
                    ]
                },
                "is_enabled": {
                    "title": "Enable flag",
                    "description": "Represent the available states.",
                    "default": true,
                    "type": "boolean"
                },
                "param": {
                    "title": "Filter Argument",
                    "description": "Represent volume image filter parameters. Refer to the module \"sitk_image_filters\" forlearning the usage of each parameters.",
                    "type": "object"
                }
            },
            "required": [
                "name",
                "param"
            ],
            "examples": [
                {
                    "enable": true,
                    "name": "UnsharpMaskImageFilter",
                    "param": {
                        "amount": 2,
                        "sigmas": 1
                    }
                },
                {
                    "name": "EnhanceVolumeImageFilter",
                    "param": {
                        "model": "models-cunet",
                        "sr_factor": 0,
                        "scale": 2
                    }
                },
                {
                    "name": "DenoisingVolumeImageFilter",
                    "param": {
                        "level": 15
                    }
                },
                {
                    "name": "BSRGanVolumeImageFilter",
                    "param": {
                        "model": "bsrganx2"
                    }
                }
            ]
        },
        "ImageFilterItems": {
            "title": "ImageFilterItems",
            "description": "An enumeration.",
            "enum": [
                "Waifu2xImageFilter",
                "ESRGANImageFilter",
                "SwinIRImageFilter",
                "SyngularImageFilter"
            ]
        },
        "EnhanceImageFilter": {
            "title": "EnhanceImageFilter",
            "type": "object",
            "properties": {
                "name": {
                    "title": "2D Image enhancement filter name.",
                    "allOf": [
                        {
                            "$ref": "#/definitions/ImageFilterItems"
                        }
                    ]
                },
                "is_enabled": {
                    "title": "Enable flag",
                    "description": "Represent the available states.",
                    "default": true,
                    "type": "boolean"
                },
                "param": {
                    "title": "Filter Argument",
                    "description": "Represent image filter function parameters. Refer to the module \"Image_enhance_filters.py\" forlearning the usage of each paramters.",
                    "type": "object"
                }
            },
            "required": [
                "name",
                "param"
            ],
            "examples": [
                {
                    "enable": true,
                    "name": "Waifu2xImageFilter",
                    "param": {
                        "sr_factor": 1,
                        "scale": 1
                    }
                }
            ]
        },
        "MarsDicomInterpolation": {
            "title": "MarsDicomInterpolation",
            "description": "Interpolate new slices to the dicom raw data. The export volume intensity would\nbe resampled from (threshold.min, threshold.max) to range (0,255) {uint8}.\n\nArgs:\n    BaseModel ([type]): [description]",
            "type": "object",
            "properties": {
                "is_enabled": {
                    "title": "Represent the flag of generating interpolation frames.",
                    "default": false,
                    "type": "boolean"
                },
                "name": {
                    "title": "Represent the interpolation resolve name. (default: CyclicGen)",
                    "default": "CyclicGen",
                    "type": "string"
                },
                "model": {
                    "title": "Represent the pre-trained model of the specific resolver.",
                    "default": "cyclicgen_large",
                    "type": "string"
                },
                "iteration": {
                    "title": "Represent the iteration times of interpolating new slices.",
                    "description": "e.g., \"2\" iterations mean interpolating \"3\" new slices. \"3\" iterations mean interpolating \"7\" new slices.",
                    "default": 1,
                    "type": "integer"
                },
                "enhance_filter": {
                    "title": "Enhance Image Filters e.g., Waifu2xImageFilter.",
                    "description": "Apply imaging filters to enhance the 2d image data. e.g., super-resolution / denoising",
                    "default": [],
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/EnhanceImageFilter"
                    }
                }
            }
        },
