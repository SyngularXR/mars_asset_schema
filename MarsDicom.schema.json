            "properties": {
                "name": {
                    "title": "Name",
                    "enum": [
                        "corrective_smooth"
                    ],
                    "type": "string"
                },
                "smooth_type": {
                    "title": "Smooth Type",
                    "default": "LENGTH_WEIGHTED",
                    "type": "string"
                },
                "factor": {
                    "title": "Factor",
                    "default": 0.6,
                    "type": "number"
                },
                "iterations": {
                    "title": "Iterations",
                    "default": 3,
