            "properties": {
                "name": {
                    "title": "Name",
                    "enum": [
                        "remesh"
                    ],
                    "type": "string"
                },
                "mode": {
                    "title": "Mode",
                    "default": "SMOOTH",
                    "type": "string"
                },
                "octree_depth": {
                    "title": "Octree Depth",
