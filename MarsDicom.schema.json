        "MeshModifier": {
            "title": "MeshModifier",
            "discriminator": {
                "propertyName": "name",
                "mapping": {
                    "remesh": "#/definitions/RemeshModifier",
                    "flatten_vertices": "#/definitions/FlattenVerticesModifier",
                    "triangulate": "#/definitions/TriangulateModifier",
                    "smooth": "#/definitions/SmoothModifier",
                    "corrective_smooth": "#/definitions/CorrectiveSmoothModifier",
                    "decimate": "#/definitions/DecimateModifier"
                }
            },
            "anyOf": [
                {
                    "$ref": "#/definitions/RemeshModifier"
                },
                {
                    "$ref": "#/definitions/FlattenVerticesModifier"
                },
                {
                    "$ref": "#/definitions/TriangulateModifier"
                },
